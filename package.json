{
	"name": "przytulanka",
	"version": "0.1.0",
	"license": "MIT",
	"author": "lasmedia <lasmedia2018@gmail.com>",
	"scripts": {
		"start": "gatsby develop",
		"build": "gatsby build",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"eslint": "eslint \"**/*.{js,jsx}\" --fix",
		"stylelint": "stylelint \"**/*.{js,css,scss}\" --fix",
		"prelint": "npm run format",
		"lint": "npm run stylelint",
		"postlint": "npm run eslint"
	},
	"dependencies": {
		"babel-plugin-styled-components": "^1.11.1",
		"gatsby": "^2.24.54",
		"gatsby-image": "^2.4.17",
		"gatsby-plugin-manifest": "^2.4.28",
		"gatsby-plugin-modal-routing": "^1.2.0",
		"gatsby-plugin-netlify-cms": "^4.3.13",
		"gatsby-plugin-react-helmet": "^3.3.10",
		"gatsby-plugin-sharp": "^2.6.32",
		"gatsby-plugin-styled-components": "^3.3.10",
		"gatsby-source-filesystem": "^2.3.28",
		"gatsby-transformer-remark": "^2.8.33",
		"gatsby-transformer-sharp": "^2.5.14",
		"leaflet": "^1.7.1",
		"netlify-cms-app": "^2.12.20",
		"netlify-cms-locales": "^1.17.2",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-leaflet": "^2.7.0",
		"react-player": "^2.6.1",
		"react-slick": "^0.27.11",
		"slick-carousel": "^1.8.1",
		"styled-components": "^5.2.0"
	},
	"devDependencies": {
		"eslint": "^7.8.1",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"gatsby-plugin-root-import": "^2.0.5",
		"husky": "^4.3.0",
		"lint-staged": "^10.3.0",
		"prettier": "^2.1.1",
		"stylelint": "^13.7.0",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard": "^20.0.0",
		"stylelint-order": "^4.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier --config .prettierrc --write",
			"stylelint --fix",
			"eslint --fix"
		],
		"*.{json,md}": [
			"prettier --config .prettierrc --write"
		],
		"*.{css,scss}": [
			"prettier --config .prettierrc --write",
			"stylelint --fix"
		]
	}
}
